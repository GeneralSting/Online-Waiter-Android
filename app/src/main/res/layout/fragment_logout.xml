<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/owner_main_bg"
    tools:context=".ownerUI.logout.LogoutFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="16dp">

        <Button
            android:id="@+id/btnOwnerLogout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/owner_nav_logout_title"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/tvOwnerPhoneChanged"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="@+id/btnOwnerLogout"
            app:layout_constraintStart_toStartOf="@+id/btnOwnerLogout"
            app:layout_constraintTop_toBottomOf="@+id/btnOwnerLogout" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>